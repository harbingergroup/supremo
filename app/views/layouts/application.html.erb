<!DOCTYPE html>
<html>
<head>
  <title>Supremo *</title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag 'jquery','rails','jquery.tablesorter.min','application' %>
  <%#= javascript_include_tag :defaults %>
  <%= csrf_meta_tag %>
 </head>
<body class=<%=controller.action_name%>>

  <%- if notice %>
    <div id="notice" class="hidden all_notice"><%= notice %></div>
  <% end %>
 <div class="j_notice hidden all_notice"></div>
   <%- if alert %>
 <div id="alert" class="hidden all_notice"><%= alert %></div>
<%- end %>
<div class="j_alert hidden all_notice"></div>
<div class="float_right"><%= render :partial=>'welcome/menu'%></div>
<div class="clear"></div>
<%= yield %>

</body>
</html>
