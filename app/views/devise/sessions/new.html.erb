<div id="dialog" class="ou_div" >
	<div class="in_div">
		<%= form_for(resource, :as => resource_name, :url => session_path(resource_name)) do |f| %>
		<div class="field">
			<%= f.label :email%>
			<%= f.email_field :email,:size=>24 %>
		</div>
		<div class="field">
			<%= f.label :password %>
			<%= f.password_field :password,:size=>24 %>
		</div>
		<% if devise_mapping.rememberable? -%>
		<div class="field">
			<%= f.check_box :remember_me %> <%= f.label :remember_me %>
		</div>
		<% end -%>
		<div class="field">
			<%= f.submit "Sign in" %>
		</div>
		<% end %>
	</div>
</div>

<div class="field">
			<%= render :partial => "devise/shared/links" %>
</div>
